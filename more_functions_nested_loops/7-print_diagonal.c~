#include "main.h"
/**
 * times_table  - Entry point
 * void - void
 * Description : print 9 times table, starting with 0
 * Return: Alway 0 (Success)
 */
void print_diagonal(int n)
{
int space, space2;
if (n <= 0)
_putchar('\n');
else
{
for (space = 1; space <= n; space++)
{
for (space2 = 1; space2 < space; space2++)
{
_putchar(' ');
}
_putchar(92);
_putchar('\n');
}
}
}    
